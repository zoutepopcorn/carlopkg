{
  "name": "carlo-pkg-es",
  "version": "0.0.1",
  "description": "es6 pkg carlo",
  "main": "app.js",
  "scripts": {
    "build": "rm -rf execute && pkg . --out-path execute",
    "parcel": "parcel build app.js --target electron --out-file es6.js --out-dir .",
    "start": "npm run parcel && node es6.js"
  },
  "bin": {
    "carlo-pkg-es": "./app.js"
  },
  "pkg": {
    "scripts": "*.js",
    "assets": "www/**/*"
  },
  "targets": [
    "node10"
  ],
  "keywords": [],
  "dependencies": {
    "carlo": "^0.9.0",
    "os-name": "^3.0.0",
    "systeminformation": "^3.54.0"
  },
  "devDependencies": {
    "pkg": "^4.3.4",
    "@pptr/testrunner": "^0.5.0"
  }
}